{
    "name": "brandgrab",
    "description": "BrandGrab",
    "homepage": "https://www.brandgrab.io/",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "engines": {
        "node": "20.x"
    },
    "scripts": {
        "dev": "next -p 4001 --turbo",
        "build": "next build",
        "build:analyze": "cross-env ANALYZE=true pnpm build",
        "postbuild": "next-sitemap --config next-sitemap.config.js",
        "start": "next start -p 4001",
        "lint": "next lint",
        "test": "playwright test"
    },
    "dependencies": {
        "@enterwell/react-hooks": "0.5.0",
        "@next/env": "14.1.0",
        "@signalco/cms-core": "workspace:*",
        "@signalco/cms-components-marketing": "workspace:*",
        "@signalco/data": "workspace:*",
        "@signalco/hooks": "workspace:*",
        "@signalco/js": "workspace:*",
        "@signalco/ui": "workspace:*",
        "@signalco/ui-primitives": "workspace:*",
        "@signalco/ui-icons": "workspace:*",
        "cheerio": "1.0.0-rc.12",
        "classix": "2.1.35",
        "next": "14.1.0",
        "next-secure-headers": "2.2.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-transition-group": "4.4.5",
        "sharp": "0.33.2"
    },
    "devDependencies": {
        "@axe-core/playwright": "4.8.3",
        "@babel/core": "7.23.7",
        "@next/bundle-analyzer": "14.1.0",
        "@playwright/test": "1.41.0",
        "@signalco/eslint-config-signalco": "workspace:*",
        "@signalco/tailwindcss-config-signalco": "workspace:*",
        "@signalco/tsconfig": "workspace:*",
        "@types/color": "3.0.6",
        "@types/node": "20.11.5",
        "@types/react": "18.2.48",
        "@types/react-dom": "18.2.18",
        "@types/sharp": "0.31.1",
        "cross-env": "7.0.3",
        "eslint": "8.56.0",
        "next-sitemap": "4.2.3",
        "postcss": "8.4.33",
        "rimraf": "5.0.5",
        "sass": "1.70.0",
        "tailwindcss": "3.4.1",
        "tailwindcss-animate": "1.0.7",
        "typescript": "5.3.3"
    },
    "nextBundleAnalysis": {
        "budget": 358400,
        "budgetPercentIncreaseRed": 10,
        "minimumChangeThreshold": 10,
        "showDetails": true,
        "skipCommentIfEmpty": true
    }
}
