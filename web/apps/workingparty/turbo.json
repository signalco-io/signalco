{
    "$schema": "https://turbo.build/schema.json",
    "extends": [
        "//"
    ],
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".next/**",
                "public/*.js"
            ],
            "env": [
                "COSMOSDB_CONNECTION_STRING",
                "OPENAI_API_KEY",
                "ACS_CONNECTION_STRING",
                "WP_JWT_SIGN_SECRET",
                "STRIPE_SECRETKEY",
                "NEXT_PUBLIC_STRIPE_PUBLISHABLE"
            ]
        }
    }
}
