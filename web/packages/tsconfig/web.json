{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Web",
    "extends": "./base.json",
    "compilerOptions": {
        "plugins": [{ "name": "next" }],
        "module": "ESNext",
        "moduleResolution": "Bundler",
        "allowJs": true,
        "jsx": "preserve",
        "noEmit": true,
        "typeRoots": [
            "../node_modules/@types",
            "../@types"
        ],
    }
}
