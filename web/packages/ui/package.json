{
    "name": "@signalco/ui",
    "version": "0.0.0",
    "private": true,
    "license": "MIT",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "npm run clean && concurrently \"npm run build:bundle\" \"npm run build:types\"",
        "build:bundle": "rollup -c",
        "build:types": "tsc --emitDeclarationOnly --declaration --project tsconfig.build.json",
        "dev": "concurrently --kill-others \"npm run dev:bundle\" \"npm run dev:types\"",
        "dev:bundle": "rollup --watch -c",
        "dev:types": "tsc --watch --emitDeclarationOnly --declaration --project tsconfig.build.json",
        "clean": "rimraf dist",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "24.1.0",
        "@rollup/plugin-node-resolve": "15.0.2",
        "@signalco/tsconfig": "workspace:*",
        "@types/react": "18.2.0",
        "@types/react-dom": "18.2.1",
        "@types/react-timeago": "4.1.3",
        "classix": "2.1.32",
        "concurrently": "8.0.1",
        "esbuild": "0.17.18",
        "glob": "10.2.2",
        "postcss": "8.4.23",
        "postcss-preset-env": "8.3.2",
        "resolve-url-loader": "5.0.0",
        "rimraf": "5.0.0",
        "rollup": "3.21.1",
        "rollup-plugin-esbuild": "5.0.0",
        "rollup-plugin-peer-deps-external": "2.2.4",
        "rollup-plugin-postcss": "4.0.2",
        "rollup-plugin-summary": "2.0.0",
        "sass": "1.62.1",
        "typescript": "5.0.4"
    },
    "dependencies": {
        "@babel/core": "7.21.5",
        "@emotion/cache": "11.10.8",
        "@emotion/react": "11.10.8",
        "@emotion/server": "11.10.0",
        "@emotion/styled": "11.10.8",
        "@mui/base": "5.0.0-alpha.127",
        "@mui/joy": "5.0.0-alpha.77",
        "@mui/system": "5.12.1",
        "@signalco/ui-icons": "workspace:*",
        "material-ui-popup-state": "5.0.8",
        "next": "13.3.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-is": "18.2.0",
        "react-timeago": "7.1.0",
        "styled-components": "5.3.10"
    },
    "peerDependencies": {
        "@babel/core": "7.21.5",
        "@signalco/ui-icons": "workspace:*",
        "next": "13.3.1",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    }
}
