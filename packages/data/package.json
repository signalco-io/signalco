{
    "name": "@signalco/data",
    "version": "0.0.0",
    "private": true,
    "sideEffects": false,
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js"
        }
    },
    "type": "module",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup src/index.ts --dts --format esm --clean --minify",
        "dev": "tsup src/index.ts --dts --format esm --clean --watch",
        "clean": "rimraf dist"
    },
    "devDependencies": {
        "@signalco/tsconfig": "*",
        "tsup": "6.5.0",
        "concurrently": "7.6.0",
        "esbuild": "0.17.4",
        "prop-types": "15.8.1",
        "rimraf": "4.1.2",
        "rollup": "3.11.0",
        "rollup-plugin-esbuild": "5.0.0",
        "rollup-plugin-summary": "2.0.0",
        "typescript": "4.9.4"
    }
}
