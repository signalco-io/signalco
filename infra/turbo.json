{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build",
        "lint"
      ]
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ]
    },
    "preview-next": {
      "dependsOn": [
        "build"
      ]
    },
    "up-next": {
      "dependsOn": [
        "build"
      ]
    }
  }
}
